<div class="beer-container">
    <div>
        <img src="{{this.image_url}}" alt="beer image">
    </div>
    <div class="beer-data">
        {{> beerpartial}}
        <h3>Food pairing</h3>
        <ul>
            {{#each this.food_pairing}}
            <li>{{this}}</li>
            {{/each}}
        </ul>
        <p><span>Brewer Tips: </span>{{this.brewers_tips}}</p>
    </div>
</div>